# Konfigurasi Database MySQL/MariaDB untuk Nginx Proxy Manager

# Password untuk root user MySQL
MYSQL_ROOT_PASSWORD=rootpassword123

# Database name
MYSQL_DATABASE=npm

# Username untuk aplikasi
MYSQL_USER=npm

# Password untuk user aplikasi
MYSQL_PASSWORD=npmuserpassword123

# Domain untuk SSL (opsional)
DOMAIN=yourdomain.com

# Email untuk Let's Encrypt (opsional)
EMAIL=your-email@example.com
